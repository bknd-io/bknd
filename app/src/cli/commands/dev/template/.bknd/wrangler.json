{
  "name": "bknd-dev",
  "main": "./worker.ts",
  "compatibility_date": "2025-10-08",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "enabled": true
  },
  "assets": {
    "binding": "ASSETS",
    "directory": "../dist/client",
    "not_found_handling": "single-page-application",
    "run_worker_first": ["!/", "/admin*", "/api*", "!/assets/*"]
  },
  "vars": {
    "ENVIRONMENT": "development"
  },
  "d1_databases": [
    {
      "binding": "DB"
    }
  ],
  "r2_buckets": [
    {
      "binding": "BUCKET"
    }
  ]
}
